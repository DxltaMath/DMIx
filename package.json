{
  "name": "@deltamath/dmpx",
  "displayName": "DeltaMath Mod & Plugin Loader Extension | DMPx",
  "version": "0.2.0",
  "description": "Mod and plugin loader for DeltaMath.",
  "author": {
    "name": "gemsvido",
    "email": "gemsvido@gmail.com"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "plasmo dev",
    "debug": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "^0.62.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "^3.6.1",
    "@types/chrome": "^0.0.208",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "chrome-types": "^0.1.175",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "manifest": {
    "author": "DxltaMath",
    "permissions": [
      "declarativeNetRequest"
    ],
    "host_permissions": [
      "https://*.deltamath.com/*"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "~rulesets/rules.json"
        }
      ]
    }
  }
}
