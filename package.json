{
  "name": "@deltamath/dmpx",
  "displayName": "DeltaMath Mod & Plugin Loader Extension | DMPx",
  "version": "0.2.0",
  "description": "Mod and plugin loader for DeltaMath.",
  "author": {
    "name": "gemsvido",
    "email": "gemsvido@gmail.com"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "plasmo dev",
    "debug": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.62.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.208",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "chrome-types": "^0.1.168",
    "prettier": "2.8.2",
    "typescript": "4.9.4"
  },
  "manifest": {
    "author": "DxltaMath",
    "permissions": [
      "declarativeNetRequest"
    ],
    "host_permissions": [
      "https://*.deltamath.com/*"
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "~rulesets/rules.json"
        }
      ]
    }
  }
}
